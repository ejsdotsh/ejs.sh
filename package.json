{
  "name": "ejs.sh",
  "version": "0.0.3",
  "description": "personal site of e.j. sahala (they/them). (infrequent) musings on coffee, networking, automation, technology, philosophy, and sometimes other stuff",
  "private": true,
  "scripts": {
    "build": "npm-run-all webpack hugo-build",
    "dev": "npm-run-all webpack --parallel webpack-watch hugo-server",
    "hugo-build": "hugo --cleanDestinationDir",
    "hugo-server": "hugo server --disableFastRender",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch"
  },
  "dependencies": {
    "@catppuccin/palette": "^0.1.8",
    "bulma": "^0.9.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
